---
import Section from "../components/Section.astro";
import { skills } from "@/cv.json";
---

<Section  title="Habilidades" >

    {
      skills.map((skill) => (
        <div class="w-36 h-18 inline-block gap-3 p-1 m-1 font-medium bg-slate-300 rounded-md text-red-900 text-center text-sm" id="skills">
          <h2>{skill.name}</h2>
        </div>
      ))
    }
  
</Section>

